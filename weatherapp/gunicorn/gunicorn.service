[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/WeatherApp/weatherapp
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/WeatherApp/weatherapp/weatherapp/weatherapp.sock weatherapp.wsgi:application

       
[Install]
WantedBy=multi-user.target